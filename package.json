{
  "name": "luiz-ipsum",
  "private": true,
  "description": "A blog about things and a really nice playground",
  "version": "1.0.0",
  "author": "Luiz Mariz",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop -H 127.0.0.1 -p 8000",
    "format": "prettier --write \"{gatsby-*.js,src/**/*.{js,jsx,json,css}}\"",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "clean": "rm -rf ./public && rm -rf ./cache",
    "deploy": "yarn clean && gatsby build && echo luizipsum.com.br > ./public/CNAME && gh-pages -d public -b master"
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.10.6",
    "gatsby": "^4.21.1",
    "gatsby-image": "^2.2.37",
    "gatsby-plugin-catch-links": "^2.1.21",
    "gatsby-plugin-feed": "^2.3.25",
    "gatsby-plugin-google-analytics": "^2.1.31",
    "gatsby-plugin-manifest": "^2.2.34",
    "gatsby-plugin-offline": "^3.0.30",
    "gatsby-plugin-react-helmet": "^3.1.18",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sharp": "^2.3.10",
    "gatsby-plugin-sitemap": "^2.2.24",
    "gatsby-plugin-styled-components": "^3.1.16",
    "gatsby-plugin-use-dark-mode": "^1.1.2",
    "gatsby-remark-autolink-headers": "^2.1.22",
    "gatsby-remark-copy-linked-files": "^2.1.33",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^3.1.39",
    "gatsby-remark-prismjs": "^3.3.29",
    "gatsby-remark-reading-time": "^1.1.0",
    "gatsby-remark-responsive-iframe": "^2.2.30",
    "gatsby-remark-smartypants": "^2.1.19",
    "gatsby-source-filesystem": "^2.1.43",
    "gatsby-transformer-remark": "^2.6.45",
    "gatsby-transformer-sharp": "^2.3.9",
    "gh-pages": "^2.2.0",
    "intersection-observer": "^0.7.0",
    "prismjs": "^1.18.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-icons": "^3.8.0",
    "react-intersection-observer": "^8.25.2",
    "react-responsive": "^8.0.1",
    "styled-components": "^4.4.1",
    "styled-media-query": "^2.1.2",
    "use-dark-mode": "^2.3.1"
